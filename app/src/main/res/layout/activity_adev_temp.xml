<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/app_main_color">

    <LinearLayout
        android:id="@+id/layout_select_1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="20dp"
        android:layout_marginBottom="10dp"
        android:background="@drawable/bg_round_10dp_2e1230"
        android:orientation="vertical"
        android:padding="10dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_vertical"
            android:orientation="horizontal">

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/ic_icon_tabbar_find_inactive" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:fontFamily="@font/gowunbatang_regular"
                android:text="@string/activity_adev_temp_0"
                android:textColor="@color/white"
                android:textSize="@dimen/text_size_body" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/btn_cb_1_1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:paddingVertical="5dp">
            <CheckBox
                android:id="@+id/cb_1_1"
                android:enabled="false"
                android:background="@drawable/bg_checkbox"
                android:button="@android:color/transparent"
                android:layout_marginRight="10dp"
                android:layout_width="20dp"
                android:layout_height="20dp"/>
            <TextView
                android:fontFamily="@font/gowunbatang_regular"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/activity_adev_temp_1"
                android:textSize="18dp"
                android:textColor="@color/white"
                android:gravity="center_vertical"/>
        </LinearLayout>
        <LinearLayout
            android:id="@+id/btn_cb_1_2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:paddingVertical="5dp">
            <CheckBox
                android:id="@+id/cb_1_2"
                android:enabled="false"
                android:background="@drawable/bg_checkbox"
                android:button="@android:color/transparent"
                android:layout_marginRight="10dp"
                android:layout_width="20dp"
                android:layout_height="20dp"/>
            <TextView
                android:fontFamily="@font/gowunbatang_regular"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/activity_adev_temp_2"
                android:textColor="@color/white"
                android:textSize="18dp"
                android:gravity="center_vertical"/>
        </LinearLayout>
        <LinearLayout
            android:id="@+id/btn_cb_1_3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:paddingVertical="5dp">
            <CheckBox
                android:id="@+id/cb_1_3"
                android:enabled="false"
                android:background="@drawable/bg_checkbox"
                android:button="@android:color/transparent"
                android:layout_marginRight="10dp"
                android:layout_width="20dp"
                android:layout_height="20dp"/>
            <TextView
                android:fontFamily="@font/gowunbatang_regular"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/activity_adev_temp_3"
                android:textColor="@color/white"
                android:textSize="18dp"
                android:gravity="center_vertical"/>
        </LinearLayout>
    </LinearLayout>
</RelativeLayout>
