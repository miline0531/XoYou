<resources>

    <declare-styleable name="SeekCircle">
        <attr name="min" format="integer" value="0" />
        <attr name="max" format="integer" value="100" />
        <attr name="progress" format="integer" value="0" />
        <attr name="startColor" format="color" />
        <attr name="endColor" format="color" />
        <attr name="inactiveColor" format="color" />
        <attr name="ringBias" format="float" />
        <attr name="sectionRatio" format="float" />
    </declare-styleable>

    <declare-styleable name="CustomGauge">
        <attr name="gaugePointSize" format="integer" />
        <attr name="gaugePointStartColor" format="color" />
        <attr name="gaugePointEndColor" format="color" />
        <attr name="gaugeStrokeColor" format="color" />
        <attr name="gaugeStrokeWidth" format="dimension" />
        <attr name="gaugeStrokeCap" format="string" />
        <attr name="gaugeStartAngle" format="integer" />
        <attr name="gaugeSweepAngle" format="integer" />
        <attr name="gaugeStartValue" format="integer" />
        <attr name="gaugeEndValue" format="integer" />
        <attr name="gaugeDividerSize" format="integer" />
        <attr name="gaugeDividerColor" format="color" />
        <attr name="gaugeDividerStep" format="integer" />
        <attr name="gaugeDividerDrawFirst" format="boolean" />
        <attr name="gaugeDividerDrawLast" format="boolean" />
    </declare-styleable>

    <declare-styleable name="circleProgress">
        <attr name="showProgressText" format="boolean" />
        <attr name="circle_progress" format="integer" />
        <attr name="secondaryProgress" format="integer" />
        <attr name="progressColor" format="color" />
        <attr name="secondaryProgressColor" format="color" />
        <attr name="backgroundColor" format="color" />
        <attr name="primaryCapSize" format="integer" />
        <attr name="secodaryCapSize" format="integer" />
        <attr name="primaryCapVisibility" format="boolean" />
        <attr name="secodaryCapVisibility" format="boolean" />
        <attr name="strokeWidth" format="dimension" />
        <attr name="textColor" format="color" />
    </declare-styleable>

</resources>